{
  "manifest_version": 2,

  "name": "Evan Time",
  "description": "Makes the background of your webpage infinitely happier.",
  "version": "1.0",

  "permissions": [
    "activeTab",
    "storage"
  ],

  "background": {
    "scripts": ["jquery-1.11.1.min.js", "background.js"],
    "persistent": false
  },

  "browser_action": {
    "default_icon": "evan.png",
    "default_title": "Evan Time"
  }
}